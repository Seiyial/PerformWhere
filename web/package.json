{
  "name": "performwhere-web",
  "version": "1.0.0",
  "description": "Where to perform!? Compare prices of regular concert hall for your performance date(s).",
  "main": "index.js",
  "repository": "https://github.com/Seiyial/PerformWhere",
  "author": "Seiyial <seiyial@syxworks.co>",
  "license": "UNLICENSED",
  "devDependencies": {
    "luxon": "^2.3.0",
    "parcel": "^2.3.2",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "sass": "^1.49.8"
  },
  "scripts": {
    "start": "cd .. && yarn run start",
    "dev": "yarn run parcel watch index.html --out-dir ../dist",
    "build": "yarn run parcel build index.html --out-dir ../dist --no-source-maps"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ]
  },
  "staticFiles": {
    "staticPath": "../docs",
    "watcherGlob": "**"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "bulma": "^0.8.0",
    "handlebars": "^4.7.7",
    "jquery": "^3.4.1",
    "material-components-web": "^4.0.0",
    "mixpanel-browser": "^2.33.1"
  }
}
