{
  "name": "performwhere-web",
  "version": "1.0.0",
  "description": "Where to perform!? Compare prices of regular concert hall for your performance date(s).",
  "main": "index.js",
  "repository": "https://github.com/Seiyial/PerformWhere",
  "author": "Seiyial <seiyial@seiyianworks.io>",
  "license": "UNLICENSED",
  "devDependencies": {
    "luxon": "^1.12.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "sass": "^1.17.4"
  },
  "scripts": {
    "start": "cd .. && yarn run start",
    "dev": "yarn run parcel watch index.html --out-dir ../dist",
    "build": "yarn run parcel build index.html --out-dir ../dist"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/*"
    ]
  },
  "staticFiles": {
    "staticPath": "../docs",
    "watcherGlob": "**"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.4",
    "handlebars": "^4.1.2",
    "jquery": "^3.4.0",
    "material-components-web": "^1.1.0"
  }
}
